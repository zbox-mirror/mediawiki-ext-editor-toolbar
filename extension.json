{
  "name": "MW_EXT_EditorToolbar",
  "version": "1.0.0",
  "author": [
    "[https://z17.dev/ z17 Dev]",
    "[https://z17.cx/ z17 CX]",
    "..."
  ],
  "url": "https://z17.dev/",
  "descriptionmsg": "mw-ext-editor-toolbar-desc",
  "license-name": "[https://choosealicense.com/licenses/mit/ MIT]",
  "type": "parserhook",
  "require": {
    "MediaWiki": ">= 1.31.0",
    "extensions": {
      "MW_EXT_Kernel": "*"
    }
  },
  "AutoloadClasses": {
    "MediaWiki\\Extension\\Z17DEV\\MW_EXT_EditorToolbar": "MW_EXT_EditorToolbar.class.php"
  },
  "Hooks": {
    "BeforePageDisplay": [
      "MediaWiki\\Extension\\Z17DEV\\MW_EXT_EditorToolbar::onBeforePageDisplay"
    ]
  },
  "MessagesDirs": {
    "MW_EXT_EditorToolbar": [
      "i18n"
    ]
  },
  "ResourceModules": {
    "ext.mw.editor-toolbar": {
      "scripts": [
        "scripts/_functions.min.js",
        "scripts/sections.min.js",
        "scripts/groups.min.js",
        "scripts/buttons.min.js"
      ],
      "position": "bottom",
      "targets": [
        "desktop",
        "mobile"
      ]
    },
    "ext.mw.editor-toolbar.styles": {
      "styles": [
        "styles/theme.css"
      ],
      "position": "top",
      "targets": [
        "desktop",
        "mobile"
      ]
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "modules",
    "remoteExtPath": "MW_EXT_EditorToolbar/modules"
  },
  "manifest_version": 2
}
